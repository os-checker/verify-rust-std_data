  "src": "fn is_null_terminated(ptr: *const c_char) -> bool {\n    let mut next = ptr;\n    let mut found_null = false;\n    while can_dereference(next) {\n        if unsafe { *next == 0 } {\n            found_null = true;\n            break;\n        }\n        next = next.wrapping_add(1);\n    }\n    if (next.addr() - ptr.addr()) >= isize::MAX as usize {\n        return false;\n    }\n    found_null\n}",
